<p>
  <strong>T Number:</strong>
  <%= @procedure.tnumber %>
</p>

<p>
  <strong>Procedure Date:</strong>
  <%= @procedure.procedureDate %>
</p>

<h2>Completed Questionnaires</h2>
<% @procedure.questionnaires.each do |questionnaire| %>
  <p>
    <strong>Username:</strong>
    <%= questionnaire.username %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= questionnaire.date %>
  </p>

   <p>
    <strong>Score:</strong>
    <%= questionnaire.score %>
  </p>
<% end %>

<h2>Add a Questionnaire Response:</h2>
<%= form_for([@procedure, @procedure.questionnaires.build]) do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :date %><br>
    <%= f.date_field :date %>
  </p>
  <p>
    <%= f.label :score %><br>
    <%= f.text_area :score %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Edit', edit_procedure_path(@procedure) %> |
<%= link_to 'Back', procedures_path %>