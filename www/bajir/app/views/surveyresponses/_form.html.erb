<%= form_for([@procedure, @procedure.surveyresponses.build]) do |f| %>
  <p>
    <%= f.label :date_of_completion %><br>
    <%= f.date_field :date %>
  </p>
  <p>
    <%= f.label :type_of_questionnaire %><br>
    <%= collection_select(:surveyresponse, :surveytemplate_id, @surveytemplates, :id, :name, {:prompt => false}) %> 

  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>