<%= form_for([@procedure, @procedure.surveyresponses.build]) do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :date %><br>
    <%= f.date_field :date %>
  </p>
  <p>
    <%= f.label :score %><br>
    <%= f.text_area :score %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>